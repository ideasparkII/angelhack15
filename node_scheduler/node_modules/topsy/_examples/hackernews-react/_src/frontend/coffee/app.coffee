define [
  'backbone'
  'eventBus'
  'routers/router'
], (Backbone, EventBus, Router) ->

  class App

    start: ->
      _App.router = new Router

      # NOTE: Backbone.Events may be sufficient
      _App.eventBus = new EventBus

      # NOTE: PushState seems to break using github pages
      Backbone.history.start()

