define [
  'react'
  'jsx/article'
], (React, ArticleComponent) ->

  ArticlesComponent = React.createClass

    componentWillMount: ->
      @articles = @props.articles?.models ? []

    render: ->
      articles = for article, index in @articles
        `<ArticleComponent key={ ++index } article={ article } />`

      `<ul>{ articles }</ul>`
